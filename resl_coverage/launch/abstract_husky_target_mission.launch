<?xml version="1.0" encoding="UTF-8"?>
<launch>

  <arg name="name" default="quadrotor"/>
  <arg name="control_args" default="0 0 5"/>

  <group ns="$(arg name)">
	  <node pkg="resl_coverage" type="controller_velocity.py" name="controller_$(arg name)" args="$(arg control_args)" output="screen"/>
	  <node pkg="resl_coverage" type="target.py" name="target_$(arg name)" args="$(arg control_args)" output="screen"/>

  </group>

</launch>
